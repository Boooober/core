\n    uint public constant TRANSFER_EVENT = 0x10;\n    uint public constant APPROVE_EVENT  = 0x20;\n\n    function transfer(address _to, uint _value) returns (bool) {\n        bytes32[] memory data = new bytes32[](3);\n        data[0] = bytes32(msg.sender);\n        data[1] = bytes32(_to);\n        data[2] = bytes32(_value);\n        if (!notify(TRANSFER_EVENT, data)) throw;\n\n        return super.transfer(_to, _value);\n    }\n\n    function transferFrom(address _from, address _to, uint _value) returns (bool) {\n        bytes32[] memory data = new bytes32[](3);\n        data[0] = bytes32(_from);\n        data[1] = bytes32(_to);\n        data[2] = bytes32(_value);\n        if (!notify(TRANSFER_EVENT, data)) throw;\n\n        return super.transferFrom(_from, _to, _value);\n    }\n\n    function approve(address _spender, uint256 _value) returns (bool) {\n        bytes32[] memory data = new bytes32[](3);\n        data[0] = bytes32(msg.sender);\n        data[1] = bytes32(_spender);\n        data[2] = bytes32(_value);\n        if (!notify(APPROVE_EVENT, data)) throw;\n\n        return super.approve(_spender, _value);\n    }\n}\n","abiDocs":[{"constant":false,"inputs":[{"name":"_spender","type":"address"},{"name":"_value","type":"uint256"}],"name":"approve","outputs":[{"name":"","type":"bool"}],"payable":false,"type":"function","signature":"approve(address,uint256)","signatureHash":"095ea7b3"},{"constant":true,"inputs":[],"name":"totalSupply","outputs":[{"name":"","type":"uint256"}],"payable":false,"type":"function","signature":"totalSupply()","signatureHash":"18160ddd"},{"constant":true,"inputs":[],"name":"TRANSFER_EVENT","outputs":[{"name":"","type":"uint256"}],"payable":false,"type":"function","signature":"TRANSFER_EVENT()","signatureHash":"1ba24a37"},{"constant":true,"inputs":[],"name":"APPROVE_EVENT","outputs":[{"name":"","type":"uint256"}],"payable":false,"type":"function","signature":"APPROVE_EVENT()","signatureHash":"1d35b913"},{"constant":false,"inputs":[{"name"